#include <stdio.h>

int main() {
    
	int x, sum=0;
	
	printf("\n");
	
    for (x=1;x<=100;x++){
    sum=sum+x; 
    printf("%i, ",x); 
    }
    
    printf("\n\nLa suma de todos los numeros del 1 al 100 es: %i",sum);
            
    return 0;
}

